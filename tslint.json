{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "interface-name": [false],
    "no-console": [true],
    "quotemark": [true, "single"],
    "trailing-comma": [
      false,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "typedef": [true, "call-signature"]
  },
  "rulesDirectory": []
}
